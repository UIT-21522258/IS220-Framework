@{
}
@model List<object>
<table class="table table-bordered">
    <tr>
        <th>Ngày giờ nhận</th>
        <th>Ngày giờ trả</th>
        <th>Số km</th>
        <th>Nội dung</th>
        <th>Chức năng</th>
    </tr>
    @foreach (var item in ViewBag.result)
    {
        <tr>
            <td><input type="date" value="@item.NgNhan" /></td>
            <td>@(item.NgTra ?? "")</td>
            <td>@item.SoKM</td>
            <td>@item.ND</td>
            <td>
                <a href="/BaoDuong/ChiTietCV?MaBD=@item.MaBD">Xem</a>
                <a href="/BaoDuong/SuaBD?MaBD=@item.MaBD">Sửa</a>
                <input type="hidden" class="MABD" value="@item.MaBD"></input>
            </td>
        </tr>
    }
</table>

