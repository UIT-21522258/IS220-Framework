@{
}
@model List<object>
<table class="table table-bordered">
    <tr>
        <th>Tên công nhân</th>
        <th>Năm sinh</th>
        <th>Nước về</th>
        <th>Số triệu chứng</th>
    </tr>
    @foreach (var item in ViewBag.result)
    {
        <tr>
            <td>@item.TenCongNhan</td>
            <td>@item.NamSinh</td>
            <td>@item.NuocVe</td>
            <td>@item.SoTrieuChung</td>
        </tr>
    }
</table>